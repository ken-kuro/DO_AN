{
  "extends": "../.eslintrc.json",
  "plugins": ["unused-imports"],
  "ignorePatterns": ["dist"],
  "env": { "node": true },
  "rules": {
    // Unused Imports
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "none",
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
